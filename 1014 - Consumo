using System;

class Program {
  public static void Main (string[] args) {

    int distancia;
    double litrosGastos, consumo;

    distancia = int.Parse(Console.ReadLine());
    litrosGastos = double.Parse(Console.ReadLine());

    consumo = distancia/litrosGastos;

    Console.WriteLine ("{0} km/l", consumo.ToString("0.000"));
  }
}
